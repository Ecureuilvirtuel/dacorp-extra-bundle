{#{% form_theme pictureForm 'DacorpExtraBundle:Form:fields.html.twig' %}#}

<form id="da-picture-upload-form" {{ form_enctype(pictureForm) }} method="post" action="">
    {{ form_errors(pictureForm) }}
    {#{{ form_row(pictureform.title) }}#}
    {#{{ form_row(pictureform.content) }}#}
    {#<div id="starts-at-wrapper" class="ui-helper-hidden">{{ form_row(pictureform.starts_at) }}</div>#}
    <div id="uploads-thumbnail-wrapper">
        <ul class="thumbnails" data-thumbnails="1">
            {# We populate the list via an API call #}
        </ul>
    </div>
    <div class="ui-helper-float-left">{{ form_row(pictureForm.images) }}</div>
    {{ form_rest(pictureForm) }}
    <button id="form-task-submit" type="submit" class="btn btn-primary ui-helper-float-right">
        {{- 'button.finish' | trans -}}
    </button>
    <div class="ui-helper-clearfix"></div>
</form>

